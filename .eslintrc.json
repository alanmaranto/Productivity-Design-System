{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "no-console": ["error", { "allow": ["error"] }],
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "no-nested-ternary": 0,
    "no-case-declarations": 0,
    "no-shadow": 0,
    "consistent-return": 0
  }
}
